<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.crm.settings.dao.UserDao">
	
	<select id="selectUserLogin" parameterType="Map" resultType="User">
        select * from tbl_user where loginAct=#{uname} and loginPwd=#{upass}
    </select>
	
</mapper>
























